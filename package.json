		"dotenv": "^16.4.7",
		"esbuild": "0.17.3",
		"esbuild-sass-plugin": "^3.3.1",
		"fast-xml-parser": "^4.5.1",
		"fflate": "^0.8.2",
		"glob-to-regexp": "^0.4.1",
		"hash-wasm": "^4.12.0",
