		"@types/diff-match-patch": "^1.0.36",
		"@types/glob-to-regexp": "^0.4.4",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^16.11.6",
		"@types/path-browserify": "^1.0.3",
		"@types/ramda": "^0.30.2",
		"@typescript-eslint/eslint-plugin": "5.29.0",
